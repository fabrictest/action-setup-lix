{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "always-update": true,
  "packages": {
    ".": {
      "exclude-paths": [
        ".github",
        "examples"
      ],
      "extra-files": [
        {
          "type": "generic",
          "path": "action.bash"
        }
      ],
      "release-type": "simple",
      "version-file": "VERSION"
    }
  },
  "plugins": [
    "sentence-case"
  ],
  "pull-request-header": ":robot: I have created a release *beep* *boop*\n"
}
